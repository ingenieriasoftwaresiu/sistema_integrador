<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Ingreso</servlet-name>
        <servlet-class>Negocio.Ingreso</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Registro</servlet-name>
        <servlet-class>Negocio.Registro</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Visualizacion</servlet-name>
        <servlet-class>Negocio.Visualizacion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CambioPWD</servlet-name>
        <servlet-class>Negocio.CambioPWD</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Ingreso</servlet-name>
        <url-pattern>/Ingreso</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Registro</servlet-name>
        <url-pattern>/Registro</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Visualizacion</servlet-name>
        <url-pattern>/Visualizacion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CambioPWD</servlet-name>
        <url-pattern>/CambioPWD</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            60
        </session-timeout>
    </session-config>
</web-app>
